# AI Visionプロバイダー選択（openai or gemini）
AI_PROVIDER=openai

# OpenAI APIキー（AI_PROVIDER=openaiの場合）
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o

# Gemini APIキー（AI_PROVIDER=geminiの場合）
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash-exp

# AI Vision専用モード（True: OCRスキップしてAI Visionのみ使用, False: ハイブリッド）
AI_VISION_ONLY=True

# デバッグモード（Trueで中間画像を保存）
DEBUG_MODE=True

# 処理済み画像の保存先
OUTPUT_DIR=./output

# OCR信頼度閾値（これ以下の場合AI Visionで再確認）
OCR_CONFIDENCE_THRESHOLD=0.7
